{
    "name": "atsmacode/framework",
    "description": "Repo with base DB, Model and Config resources",
    "type": "project",
    "authors": [
        {
            "name": "Mark Ramage",
            "email": "mramage@hotmail.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Atsmacode\\Framework\\": "app/",
            "Atsmacode\\Framework\\Migrations\\": "database/migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atsmacode\\Framework\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "doctrine/dbal": "^3.5",
        "laminas/laminas-config-aggregator": "^1.12",
        "symfony/console": "5.4.* || ^6.0 || ^7.0",
        "laminas/laminas-servicemanager": "^3.19",
        "monolog/monolog": "^1.22 || ^2.0 || ^3.0"
    }
}
